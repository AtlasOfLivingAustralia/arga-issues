{
    "retrieveType": "url",
    "download": {
        "files": [
            {
                "url": "https://ftp.ncbi.nlm.nih.gov/biosample/biosample_set.xml.gz",
                "name": "biosample_set.xml.gz"
            }
        ]
    },
    "processing": {
        "final": [
            {
                "path": "lib/tools/zipping.py",
                "function": "extract",
                "args": [
                    "{INPATH}",
                    "{OUTDIR}"
                ],
                "output": "biosample_set.xml"
            },
            {
                "path": "tools/processing/xmlProcess.py",
                "function": "process",
                "args": [
                    "{INPATH}",
                    "{OUTPATH}"
                ],
                "kwargs": {
                    "subfileRows": 150000,
                    "compressChild": [
                        "Paragraph",
                        "Attribute",
                        "Model",
                        "Id",
                        "Link",
                        "Synonym"
                    ],
                    "collectionExtract": {
                        "Attribute": {
                            "attribute_name": {
                                "collection date": "collection date",
                                "specimen_voucher": "specimen voucher",
                                "sex": "sex",
                                "developmental stage": "developmental stage",
                                "tissue": "tissue",
                                "isolate": "isolate",
                                "lat_lon": "lat long",
                                "geo_loc_name": "geo loc name"
                            }
                        }
                    }
                },
                "output": "biosample.csv"
            }
        ]
    },
    "conversion": {
        "mapID": 109194600,
        "augment": [
            {
                "path": "sourceProcessing/ncbi.py",
                "function": "augmentBiosample"
            }
        ]
    }
}
